{
  "name": "challenge",
  "version": "0.7.0",
  "private": true,
  "engines": {
    "node": "4.2.x"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "test:frontend": "node_modules/karma/bin/karma start test/client/karma.conf.js",
    "test:backend": "node_modules/mocha/bin/mocha --compilers js:babel-core/register test/{server,shared}/**/*.js",
    "test": "npm run test:backend & npm run test:frontend",
    "start": "./node_modules/grunt-cli/bin/grunt build; node ./build/app.js",
    "live": "./node_modules/grunt-cli/bin/grunt",
    "debug": "./node_modules/grunt-cli/bin/grunt build; DEBUG=true node --debug-brk=5858 ./build/app.js"
  },
  "dependencies": {
    "babel": "5.8.34",
    "babel-core": "6.4.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babelify": "7.2.0",
    "chai": "3.3.0",
    "express": "4.13.3",
    "flux": "2.1.1",
    "glicko2": "0.8.4",
    "grunt": "0.4.5",
    "grunt-babel": "5.0.3",
    "grunt-browserify": "4.0.1",
    "grunt-cli": "0.1.13",
    "grunt-concurrent": "2.0.3",
    "grunt-contrib-clean": "0.6.0",
    "grunt-contrib-jshint-jsx": "0.10.4",
    "grunt-contrib-less": "1.0.1",
    "grunt-contrib-watch": "0.6.1",
    "grunt-karma": "0.12.1",
    "grunt-nodemon": "0.4.0",
    "grunt-simple-mocha": "0.4.0",
    "grunt-sync": "0.4.1",
    "karma": "0.13.10",
    "karma-browserify": "4.3.0",
    "karma-mocha": "0.2.0",
    "karma-mocha-reporter": "1.1.1",
    "karma-phantomjs-launcher": "0.2.1",
    "load-grunt-tasks": "3.3.0",
    "lodash": "3.10.1",
    "mocha": "2.3.3",
    "mocha-sinon": "1.1.4",
    "mockery": "1.4.0",
    "phantomjs": "1.9.18",
    "react": "0.13.3",
    "rimraf": "2.4.4",
    "sinon": "1.17.1",
    "socket.io-client": "1.3.7",
    "uuid": "2.0.1",
    "validate.js": "0.8.0",
    "ws": "0.8.0"
  }
}
